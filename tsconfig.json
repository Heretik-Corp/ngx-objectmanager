{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "downlevelIteration": true,
    "importHelpers": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "ngxobjectmanager": [
        "dist/ngxobjectmanager",
        "dist/ngxobjectmanager"
      ],
      "ngxobjectmanager/*": [
        "dist/ngxobjectmanager/*",
        "dist/ngxobjectmanager/*"
      ]
    }
  }
}